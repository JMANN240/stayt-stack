[package]
name = "{{project-name}}-frontend"
version = "0.1.0"
edition = "2024"

[dependencies]
ab_glyph = "0.2.31"
base64 = "0.22.1"
getrandom = { version = "0.3.3", features = ["wasm_js"] }
glam = { workspace = true }
gloo-net = "0.6.0"
gloo-storage = "0.3.0"
image = { workspace = true }
jwt = { workspace = true }
{{project-name}}-lib = { path = "../{{project-name}}-lib" }
render-agnostic = { workspace = true, features = ["image"] }
ron = { workspace = true }
serde = { workspace = true }
serde_json = "1.0.143"
serde_plain = "1.0.2"
wasm-bindgen-futures = "0.4.50"
web-sys = { version = "0.3.77", features = ["console", "HtmlFormElement", "HtmlDivElement"] }
yew = { version = "0.21.0", features = ["csr"] }
yew-router = "0.18.0"
